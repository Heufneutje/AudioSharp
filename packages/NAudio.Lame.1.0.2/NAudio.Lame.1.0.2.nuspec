<?xml version="1.0"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>NAudio.Lame</id>
    <version>1.0.2</version>
    <authors>Corey Murtagh</authors>
